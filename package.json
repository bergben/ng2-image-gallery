{
    "name": "ng2-image-gallery",
    "version": "0.3.15",
    "description": "Basic Angular 2 image gallery",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bergben/ng2-image-gallery.git"
    },
    "license": "MIT",
    "module": "./dist/ng2-image-gallery.js",
    "main": "./dist/bundles/ng2-image-gallery.umd.js",
    "typings": "./dist/ng2-image-gallery.d.ts",
    "scripts": {
        "transpile": "ngc",
        "package": "rollup -c",
        "minify": "uglifyjs dist/bundles/ng2-image-gallery.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/ng2-image-gallery.umd.min.js",
        "build": "npm run transpile && npm run package && npm run minify"
    },
    "author": {
        "name": "Benedikt Berger",
        "email": "benedikt.berger@live.de"
    },
    "keywords": [
        "angular",
        "angular2",
        "image",
        "gallery",
        "lightbox",
        "photo"
    ],
    "dependencies": {
        "@angular/material": "2.0.0-beta.2",
        "ng2-dragula": "^1.2.2"
    },
    "peerDependencies": {
        "@angular/core": "^2.0.0",
        "@angular/common": "^2.0.0"
    },
    "devDependencies": {
        "@angular/common": "2.4.8",
        "@angular/compiler": "^2.4.8",
        "@angular/compiler-cli": "^2.4.8",
        "@angular/core": "2.4.8",
        "@angular/forms": "^2.4.8",
        "@angular/http": "^2.4.8",
        "@angular/platform-browser": "2.4.8",
        "@angular/platform-browser-dynamic": "2.4.8",
        "@angular/router": "3.4.8",
        "@types/node": "^7.0.5",
        "core-js": "^2.4.1",
        "reflect-metadata": "^0.1.3",
        "rollup": "^0.41.4",
        "rxjs": "5.1.1",
        "tslint": "^4.4.2",
        "typescript": "^2.1.6",
        "uglify-js": "^2.7.5",
        "zone.js": "^0.7.7"
    },
    "bugs": {
        "url": "https://github.com/bergben/ng2-image-gallery/issues"
    },
    "homepage": "https://github.com/bergben/ng2-image-gallery#readme"
}